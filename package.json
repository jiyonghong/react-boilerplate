{
  "name": "chatie",
  "version": "0.1.0",
  "description": "Chatie",
  "main": "index.js",
  "scripts": {
    "dev:start": "npm run clean && npm run dev:server:prepare && run-p dev:client:build dev:server:build dev:server:start",
    "dev:client:build": "node ./webpack/client/webpack-dev-server.babel.js",
    "dev:server:build": "webpack --watch --config ./webpack/server/development.babel.js --colors --display-error-details",
    "dev:server:start": "nodemon ./start.babel.js --watch ./dist/server",
    "dev:server:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "prod:start": "NODE_ENV=production node ./start.babel.js",
    "prod:build": "npm run clean && run-s prod:client:build prod:server:build",
    "prod:client:build": "NODE_ENV=production webpack --config ./webpack/client/production.babel.js --colors --display-error-details",
    "prod:server:build": "NODE_ENV=production webpack --config ./webpack/server/production.babel.js --colors --display-error-details",
    "clean": "rimraf dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "delay": "^2.0.0",
    "dotenv": "^5.0.1",
    "dotenv-expand": "^4.2.0",
    "express": "^4.16.2",
    "hpp": "^0.2.2",
    "loadable-components": "^1.1.1",
    "nodemon": "^1.15.1",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0-rc.0",
    "react-loadable": "^5.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "serialize-javascript": "^1.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-css-modules": "^3.3.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
